<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">Casa</th>
      <th scope="col">Nombre</th>
      <th scope="col">Apellido</th>
      <th scope="col">Placas</th>
      <th scope="col">Fecha visita</th>
    </tr>
  </thead>
  <tbody>
        {{#each visits}}
            <tr>
            <th scope="row">{{this.house_number}}</th>
            <td>{{this.visit_fname}}</td>
            <td>{{this.visit_lname}}</td>
            <td>{{this.car_plates}}</td>
            <td>{{moment this.visit_date format="DD-MM-YYYY"}}</td>
            </tr>
        {{/each}}
  </tbody>
</table>


